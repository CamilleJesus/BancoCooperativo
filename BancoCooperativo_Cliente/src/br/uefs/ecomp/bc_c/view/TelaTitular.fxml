<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane fx:id="telaTitular" animated="false" collapsible="false" prefHeight="200.0" prefWidth="200.0" text="Adicionar Titular" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.uefs.ecomp.bc_c.view.TelaController">
   <content>
      <AnchorPane fx:id="paneTitular" prefHeight="200.0" prefWidth="200.0">
         <children>
            <TextField fx:id="fieldNomeTitular" focusTraversable="false" layoutX="26.0" layoutY="43.0" promptText="Nome Completo..." />
            <TextField fx:id="fieldCpfTitular" focusTraversable="false" layoutX="26.0" layoutY="81.0" promptText="CPF..." />
            <Button fx:id="buttonOkTitular" focusTraversable="false" layoutX="143.0" layoutY="123.0" mnemonicParsing="false" onAction="#clicarAdicionarTelaTitular" text="Ok" />
            <Hyperlink fx:id="linkSairTitular" focusTraversable="false" layoutX="26.0" layoutY="124.0" onAction="#clicarSairTelaTitular" text="Sair" />
         </children>
      </AnchorPane>
   </content>
</TitledPane>
